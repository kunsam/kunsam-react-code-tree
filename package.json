{
  "name": "kunsam-react-code-tree",
  "description": "vscode extension with tree provider tool for react app coder",
  "repository": "https://github.com/kunsam/kunsam-react-code-tree.git",
  "version": "0.15.0",
  "publisher": "kunsam",
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:kReactCodeTree",
    "onCommand:kReactCodeTree.gotoRelative",
    "onCommand:extension.selectBracket"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "kReactCodeTree-explorer",
          "title": "react代码树",
          "icon": "media/dep.svg"
        }
      ]
    },
    "views": {
      "kReactCodeTree-explorer": [
        {
          "id": "kReactCodeTree",
          "name": "k React Code Tree"
        }
      ]
    },
    "commands": [
      {
        "command": "kReactCodeTree.refresh",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "kReactCodeTree.insertAfter",
        "title": "Insert After"
      },
      {
        "command": "extension.selectBracket",
        "title": "select bracket"
      },
      {
        "command": "kReactCodeTree.gotoRelative",
        "title": "Go to Relative import"
      },
      {
        "command": "kReactCodeTree.insertIn",
        "title": "Insert In"
      },
      {
        "command": "kReactCodeTree.editNode",
        "title": "Edit"
      },
      {
        "command": "kReactRouterTree.SearchRouter",
        "title": "Search Router"
      },
      {
        "command": "extension.getKReactNodeCode",
        "title": "* Get kReactCodeTree Node Code"
      },
      {
        "command": "extension.addOpenAsideToContextMenu",
        "title": "* Open Definition Aside"
      },
      {
        "command": "extension.getFileAppUrl",
        "title": "* Get File App Url"
      },
      {
        "command": "kReactRouterTree.showFileParentsInPick",
        "title": "show file parents"
      }
    ],
    "keybindings": [
      {
        "command": "extension.selectBracket",
        "key": "alt+a",
        "mac": "alt+a",
        "when": "editorTextFocus"
      },
      {
        "command": "kReactCodeTree.gotoRelative",
        "mac": "cmd+i cmd+i",
        "key": "ctrl+i ctrl+i",
        "when": "editorTextFocus"
      },
      {
        "command": "kReactRouterTree.SearchRouter",
        "mac": "cmd+o cmd+o",
        "key": "ctrl+o ctrl+o"
      },
      {
        "command": "kReactRouterTree.showFileParentsInPick",
        "mac": "cmd+4 cmd+4",
        "key": "ctrl+4 ctrl+4",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "commandPalette": [],
      "view/title": [
        {
          "command": "kReactCodeTree.refresh",
          "when": "view == kReactCodeTree"
        }
      ],
      "view/item/context": [
        {
          "command": "kReactCodeTree.insertAfter",
          "when": "view == kReactCodeTree"
        },
        {
          "command": "kReactCodeTree.insertIn",
          "when": "view == kReactCodeTree"
        },
        {
          "command": "kReactCodeTree.editNode",
          "when": "view == kReactCodeTree"
        }
      ],
      "editor/context": [
        {
          "when": "editorTextFocus",
          "command": "extension.getKReactNodeCode",
          "group": "navigation@7"
        },
        {
          "when": "editorTextFocus",
          "command": "extension.addOpenAsideToContextMenu",
          "group": "navigation@6"
        },
        {
          "when": "editorTextFocus",
          "command": "extension.getFileAppUrl",
          "group": "navigation@5"
        }
      ]
    },
    "configuration": []
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "add-vscode-type-on-master": "npx vscode-dts master",
    "publish": "vsce publish minor",
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/node": "^10.12.21",
    "@types/webpack": "^4.39.4",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {}
}
